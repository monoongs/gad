/*BODY*/

.carousel-inner > .item > img,
.carousel-inner > .item > a > img {
      width: 100%;
	  height:50%;
      margin: auto;
  }

  .person {
      border: 10px solid transparent;
      margin-bottom: 25px;
      width: 80%;
      height: 80%;
      opacity: 0.7;
  }
  .person:hover {
      border-color: #f1f1f1;
  }

 .cover {
	
	width: 100%;
	height: 150px;
}

.cover2 {
	width:245px;
	height:150px;
}

.covers {
	width:700px;
	height:100%;
}

.feed h1{
	text-align:left;
	text-decoration:underline;
	color:#d9241f;
}

.feed h3{
	text-align:left;
	font-weight:bold;
}

.feed h4{
	text-align:right;
	font-weight:lighter;
